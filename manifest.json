{

  "manifest_version": 2,
  "name": "coinFlip",
  "version": "1.0",

  "description": "Flip a coin.",

  "icons": {
    
  },
"permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/roll.png"
  
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["coinFlip.js"]
    }
  ]

}
